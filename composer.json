{
    "name": "hughcube/guzzle-http",
    "description": "",
    "license": "MIT",
    "authors": [
        {
            "name": "hugh.li",
            "email": "hugh.li@foxmail.com"
        }
    ],
    "require": {
        "php": ">=7.1",
        "ext-json": "*",
        "guzzlehttp/guzzle": "*"
    },
    "require-dev": {
        "phpstan/phpstan": "*",
        "squizlabs/php_codesniffer": "*",
        "mockery/mockery": "*",
        "phpunit/phpunit": "*"
    },
    "autoload": {
        "psr-4": {
            "HughCube\\GuzzleHttp\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "HughCube\\GuzzleHttp\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit",
        "phpstan": "phpstan analyse -vvv",
        "check-style": "phpcs -p --standard=PSR2 src/ -v",
        "fix-style": "phpcbf -p --standard=PSR2 src/ -v"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
